{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "alwaysAllow": [
        "browser_navigate"
      ]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/dice-wizard/dev/rebrand/"
      ],
      "disabled": false,
      "alwaysAllow": [
        "read_file",
        "read_text_file"
      ]
    }
  }
}